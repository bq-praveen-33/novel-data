                  {
                    "type": "text",
                    "marks": [
                      {
                        "type": "bold"
                      }
                    ],
                    "text": "Primitive Types"
                  },
                  {
                    "type": "text",
                    "text": ":"
                  }
                ]
              },
              {
                "type": "bulletList",
                "attrs": {
                  "tight": true
                },
                "content": [
                  {
                    "type": "listItem",
                    "content": [
                      {
                        "type": "paragraph",
                        "content": [
                          {
                            "type": "text",
                            "marks": [
                              {
                                "type": "bold"
                              }
                            ],
                            "text": "String"
                          },
                          {
                            "type": "text",
                            "text": ": Represents a sequence of characters, e.g., "
                          },
                          {
                            "type": "text",
                            "marks": [
                              {
                                "type": "code"
                              }
                            ],
                            "text": "\"Hello, World!\""
